{
	"Align": {
		"prefix": "ali",
		"body": [
			"\\begin{align*}",
			"    $0",
			"\\end{align*}"
		]
	},

	"Numbered align": {
		"prefix": "nali",
		"body": [
			"\\begin{align}",
			"    $0",
			"\\end{align}"
		]
	},

	"Equation": {
		"prefix": "eq",
		"body": [
			"\\begin{equation*}",
			"    $0",
			"\\end{equation*}"
		]
	},

	"Numbered equation": {
		"prefix": "neq",
		"body": [
			"\\begin{equation}",
			"    $0",
			"\\end{equation}"
		]
	},

	"Enumerate": {
		"prefix": "enum",
		"body": [
			"\\begin{enumerate}[$1]",
			"\\item $0",
			"\\end{enumerate}"
		]
	},

	"Itemize": {
		"prefix": "item",
		"body": [
			"\\begin{itemize}",
			"\\item $0",
			"\\end{itemize}"
		]
	},

	"Left-right parenthesis": {
		"prefix": "lrp",
		"body": [
			"\\left( $1 \\right)$0"
		]
	},

	"Left-right bracket": {
		"prefix": "lrb",
		"body": [
			"\\left[ $1 \\right]$0"
		]
	},

	"Left-right braces": {
		"prefix": "lrB",
		"body": [
			"\\left\\{ $1 \\right\\}$0"
		]
	},

	"Left-right vertical": {
		"prefix": "lrv",
		"body": [
			"\\left| $1 \\right|$0"
		]
	},

	"Left-right double vertical": {
		"prefix": "lrV",
		"body": [
			"\\left\\lVert $1 \\right\\lVert$0"
		]
	},

	"Left-right tripe vertical": {
		"prefix": "lrW",
		"body": [
			"\\lWert $1 \\lWert$0"
		]
	},

	"Left-right angle": {
		"prefix": "lra",
		"body": [
			"\\left< $1 \\right>$0"
		]
	},

	"Left-right floor": {
		"prefix": "lrf",
		"body": [
			"\\left\\lfloor $1 \\right\\rfloor)$0"
		]
	},

	"Left-right ceiling": {
		"prefix": "lrc",
		"body": [
			"\\left\\lceil $1 \\right\\rceil$0"
		]
	},

	"Set": {
		"prefix": "set",
		"body": [
			"\\left\\{ $1 \\right\\}$0"
		]
	},

    "Claim": {
		"prefix": "clm",
		"body": [
			"\\begin{claim}",
			"$0",
			"\\end{claim}"
		]
	},

	"Theorem": {
		"prefix": "thm",
		"body": [
			"\\begin{theorem}",
			"$0",
			"\\end{theorem}"
		]
	},

	"Lemma": {
		"prefix": "lem",
		"body": [
			"\\begin{lemma}",
			"$0",
			"\\end{lemma}"
		]
	},

	"Corollary": {
		"prefix": "cor",
		"body": [
			"\\begin{corollary}",
			"$0",
			"\\end{corollary}"
		]
	},

	"Proposition": {
		"prefix": "prop",
		"body": [
			"\\begin{proposition}",
			"$0",
			"\\end{proposition}"
		]
	},

	"Proof": {
		"prefix": "prf",
		"body": [
			"\\begin{proof}",
			"$0",
			"\\end{proof}"
		]
	},

	"Section": {
		"prefix": "sect",
		"body": [
			"\\section{$1}",
			"",
			"$0"
		]
	},

	"Subsection": {
		"prefix": "subsect",
		"body": [
			"\\subsection{$1}",
			"",
			"$0"
		]
	},

	"Matrix": {
		"prefix": "mat",
		"body": [
			"\\begin{${1:p}matrix}",
			"    $0",
			"\\end{${1:p}matrix}"
		]
	},

	"Sum": {
		"prefix": "sum",
		"body": "\\sum_{$1}^{$2} $0"
	},

	"Product": {
		"prefix": "prod",
		"body": "\\prod_{$1}^{$2} $0"
	},

	"Limit": {
		"prefix": "lim",
		"body": "\\lim_{$1} $0"
	},

	"Integral": {
		"prefix": "int",
		"body": "\\int_{$1} $0"
	},

	"Dashed integral": {
		"prefix": "dint",
		"body": "\\dint_{$1} $0"
	},

	"Differential": {
		"prefix": "dd",
		"body": "\\dd $0"
	},

	"Mathbb": {
		"prefix": "mbb",
		"body": "\\mathbb{$1}$0"
	},

	"Mathrm": {
		"prefix": "mrm",
		"body": "\\mathrm{$1}$0"
	},

	"Mathsf": {
		"prefix": "msf",
		"body": "\\mathsf{$1}$0"
	},

	"Mathcal": {
		"prefix": "mcal",
		"body": "\\mathcal{$1}$0"
	},

	"Mathfrak": {
		"prefix": "mfrak",
		"body": "\\mathfrak{$1}$0"
	},

	"Mathbf": {
		"prefix": "mbf",
		"body": "\\mathbf{$1}$0"
	},

	"Mathbm": {
		"prefix": "mbm",
		"body": "\\bm{$1}$0"
	},

	"Mathscr": {
		"prefix": "mscr",
		"body": "\\mathscr{$1}$0"
	},

	"Tilde": {
		"prefix": "tilde",
		"body": "\\tilde{$1}$0"
	},

	"Hat": {
		"prefix": "hat",
		"body": "\\hat{$1}$0"
	},

	"Bar": {
		"prefix": "bar",
		"body": "\\bar{$1}$0"
	},

	"Overline": {
		"prefix": "ol",
		"body": "\\overline{$1}$0"
	},

	"Today": {
		"prefix": "today",
		"body": "$CURRENT_MONTH_NAME $CURRENT_DATE, $CURRENT_YEAR"
	}
}